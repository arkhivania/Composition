<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Composition.Demo.HelloTool.Views.HelloView"
             x:ClassModifier="internal">
  <Grid ColumnDefinitions="*,*">
    <!-- <Ellipse Fill="Red" Height="200"/> -->
    <TextBlock Text="{Binding Counter.Value}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Button Command="{Binding IncrementCommand}" Grid.Column="1" Content="Increment" HorizontalAlignment="Center" VerticalAlignment="Center"/>
  </Grid>
  
</UserControl>
